{
  "expo": {
    "name": "GAIN",
    "slug": "GAIN",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/logos/gain-black-logo.png",
    "userInterfaceStyle": "automatic",
    "splash": {
      "image": "./assets/logos/gain_splash.png",
      "resizeMode": "contain"
    },
    "assetBundlePatterns": ["**/*"],
    "ios": {
      "supportsTablet": true
    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./assets/adaptive-icon.png",
        "backgroundColor": "#ffffff"
      }
    },
    "web": {
      "favicon": "./assets/favicon.png"
    },
    "plugins": ["expo-font"]
  }
}
